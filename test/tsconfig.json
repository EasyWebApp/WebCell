{
    "compilerOptions": {
        "sourceMap": true,
        "strict": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "module": "ESNext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "target": "es5",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "jsx": "react",
        "jsxFactory": "createCell"
    },
    "include": ["./**/*", "../source/**/*"]
}
