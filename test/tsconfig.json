{
    "compilerOptions": {
        "target": "ES5",
        "sourceMap": true,
        "downlevelIteration": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "jsx": "react",
        "jsxFactory": "createCell",
        "lib": ["ES2019", "DOM"]
    },
    "include": ["./**/*", "../source/**/*"]
}
