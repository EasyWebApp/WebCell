    },
    "dependencies": {
        "mobx": "^5.15.4",
        "web-cell": "^2.0.0"
    },
    "devDependencies": {
        "@types/jest": "^25.2.1",
        "@webcomponents/webcomponentsjs": "^2.4.3",
        "jest": "^26.0.1",
        "microbundle": "^0.11.0",
        "ts-jest": "^25.5.0",
        "typescript": "^3.8.3"
    },
    "jest": {
        "preset": "ts-jest"
    }
