{
    "source": "./source",
    "destination": "./docs",
    "plugins": [
        {
            "name": "esdoc-standard-plugin",
            "option": {
                "accessor": {
                    "access": ["public", "protected"],
                    "autoPrivate": false
                },
                "test": {
                    "source": "./test",
                    "interfaces": ["describe", "it"],
                    "includes": ["\\.test\\.js$"]
                },
                "manual": {
                    "files": [
                        "./manual/Integration.md"
                    ]
                }
            }
        },
        {
            "name": "esdoc-external-webapi-plugin",
            "option": {
                "enable": true
            }
        },
        {
            "name": "esdoc-ecmascript-proposal-plugin",
            "option": {
                "all": true
            }
        }
    ]
}
